{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "sourceMap": true,
      "downlevelIteration": true,
  
      "strict": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
  
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "outDir": "./dist"
    }
  }
  